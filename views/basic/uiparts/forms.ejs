                                                        options.innerHTML = "";
                                                        countries.forEach(country => {
                                                        let isSelected = country === selectedCountry ? "selected" : "";
                                                        let li = `&lt;li onclick="updateName(this)" class="${isSelected}"&gt;${country}&lt;/li&gt;`;
